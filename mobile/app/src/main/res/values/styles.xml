<?xml version="1.0" encoding="utf-8"?>
<resources><style name="AdminCardStyle">
    <item name="android:layout_width">0dp</item>
    <item name="android:layout_height">140dp</item>
    <item name="android:layout_margin">8dp</item>
    <item name="android:layout_columnWeight">1</item>
    <item name="android:clickable">true</item>
    <item name="android:focusable">true</item>
    <item name="android:foreground">?attr/selectableItemBackground</item>
    <item name="cardCornerRadius">20dp</item>
    <item name="cardElevation">6dp</item>
    <item name="android:backgroundTint">#DCEED3</item>
</style><style name="InfoRowStyle">
    <item name="android:layout_width">match_parent</item>
    <item name="android:layout_height">wrap_content</item>
    <item name="android:orientation">horizontal</item>
    <item name="android:paddingVertical">8dp</item>
    <item name="android:gravity">center_vertical</item>
</style>

    <style name="LabelStyle">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">#777777</item>
        <item name="android:textAllCaps">true</item>
    </style>

    <style name="ValueStyle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#1A1A1A</item>
    </style>
    <style name="RatingBar" parent="Theme.AppCompat">
        <item name="colorControlNormal">@color/brown_black</item>
        <item name="colorControlActivated">@color/accent_green</item>
    </style>
</resources>

